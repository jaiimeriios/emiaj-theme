"editor.tokenColorCustomizations": {
	"textMateRules": [{
		"scope": [
			"comment"
		],
			"settings": {
				"foreground": "#529797"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.operator",
				"constant.other.color",
				"punctuation",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#5FB3B3"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#97d391"
			}
		},
		// HTML
		{
			"name": "[CSS] - Entity Tag Name",
			"scope": [
				"entity.name.tag",
				"source.css entity.name",
				"source.css entity.name.tag",
				"source.scss entity.name.tag",
				"source.css entity.name.tag entity.name.tag",
				"source.scss entity.name.tag entity.name.tag"
			],
			"settings": {
				"foreground": "#cf5966"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#b576ce",
				"fontStyle": "italic"
			}
		},
		// CSS
		{
			"name": "Attribute class and id",
			"scope": "entity.other.attribute-name.class.css, entity.other.attribute-name.id.css",
			"settings": {
				"foreground": "#2d94e7"
			}
		},
		{
			"name": "support.constant.property-name.scss",
			"scope": "support.type.property-name.scss,support.type.property-name.css",
			"settings": {
				"foreground": "#bd72d4"
			}
		},
		{
			"name": "[CSS] - Constant",
			"scope": [
				"source.css constant",
				"source.css support.constant"
			],
			"settings": {
				"foreground": "#2bbac5"
			}
		},
		{
			"name": "css property-name",
			"scope": "constant.numeric.css, meta.property-value.scss, source.css.scss, keyword.other.unit.vw.css, keyword.other.unit.vh.css, keyword.other.unit.px.css, constant.numeric.css, meta.property-value.scss, source.css.scss",
			"settings": {
				"foreground": "#2bbac5"
			}
		},
		// JS
		{
			"name": "Variable",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#fafafa"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"storage.type.class.js"
			],
			"settings": {
				"foreground": "#b15fca"
			}
		},
		{
			"name": "Function, Special Method, Block Level",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"meta.block-level"
			],
			"settings": {
				"foreground": "#0e83e2"
			}
		},
		// markdown
		{
			"name": "[MARKDOWN] - Heading Name Section",
			"scope": [
				"entity.name.section.markdown",
				"markup.heading.setext.1.markdown",
				"markup.heading.setext.2.markdown"
			],
			"settings": {
				"foreground": "#c5a5c5",
				"fontStyle": "bold"
			}
		},
		{
			"name": "[MARKDOWN] - Paragraph",
			"scope": "meta.paragraph.markdown",
			"settings": {
				"foreground": "#d8dee9"
			}
		},
		{
			"name": "[MARKDOWN] - Link Address",
			"scope": [
				"markup.underline.link.markdown",
				"markup.underline.link.image.markdown",
				"fenced_code.block.language",
				"markup.inline.raw.markdown"
			],
			"settings": {
				"foreground": "#5a9bcf"
			}
		},
	]
}
